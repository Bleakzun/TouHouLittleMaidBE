          "inventory_size": 6,
          "private": false
        }
      },
      "type:small":{
        "minecraft:skin_id": {"value": 1},
        "minecraft:inventory":{
          "container_type": "inventory",
          "inventory_size": 12,
          "private": false
        }
      },
      "type:medium":{
        "minecraft:skin_id": {"value": 2},
        "minecraft:inventory":{
          "container_type": "inventory",
          "inventory_size": 24,
          "private": false
        }
      },
      "type:big":{
        "minecraft:skin_id": {"value": 3},
        "minecraft:inventory":{
          "container_type": "inventory",
          "inventory_size": 36,
          "private": false
        }
      }
    },
