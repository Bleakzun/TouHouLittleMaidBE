{
  "format_version": "1.18.10",
  "minecraft:entity": {
    "description": {
      "identifier": "thlmm:maid_basic",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },

    "component_groups": {
      "thlmm:maid_wild": {
        "minecraft:behavior.panic": {
          "priority": 1,
          "speed_multiplier": 1.25
        },
        "minecraft:tameable": {
          "probability": 0.8,
          "tame_items": "cake",
          "tame_event": {
            "event": "on_tamed",
            "target": "self"
          }
        }
      },
      "thlmm:maid_tame": {
        "minecraft:sittable": {},
        "minecraft:inventory":{
          "container_type": "inventory",
          "inventory_size": 27,
          "private": false,
          "restrict_to_owner": true
        },
        "minecraft:is_tamed": {
        },
        "minecraft:health": {
          "value": 20,
          "max": 20
        },
        "minecraft:damage_sensor": { 
          "triggers": [
            {
              "cause": "attack",
              "on_damage": {
                "filters": {
                  "all_of": [
                    {"test": "is_family","subject": "damager","value": "player"},
                    {"test": "is_owner","subject": "damager","value": true}
                  ]
                },
                "event": "thlmm:omi",
                "target": "self"
              },
              "deals_damage": false
            }
          ]
        }
      },
      "thlmm:maid_tame_sit":{
        "minecraft:entity_sensor": {
          "sensor_range": 6,
          "relative_range": false,
          "minimum_count": 1,
          "maximum_count": 2,
          "event_filters": {
            "all_of": [
              {"test": "is_owner","subject": "other","value": true},
              {"test": "is_sneaking","subject": "other","value": false}
            ]
          },
          "event": "inventory_mode"
        }
      },
      "thlmm:maid_tame_inventory":{
        "minecraft:entity_sensor": {
          "sensor_range": 6,
          "relative_range": false,
          "minimum_count": 1,
          "maximum_count": 2,
          "event_filters": {
            "all_of": [
              {"test": "is_owner","subject": "other","value": true},
              {"test": "is_sneaking","subject": "other","value": true}
            ]
          },
          "event": "sit_mode"
        }
      },

      "status:follow_on_tame":{
        "minecraft:behavior.follow_owner": {
          "priority": 1,
          "speed_multiplier": 1.0,
          "start_distance": 1,
          "stop_distance": 1,
          "can_teleport": true
        },
        "minecraft:entity_sensor": {
          "sensor_range": 1,
          "relative_range": false,
          "minimum_count": 1,
          "maximum_count": 2,
          "event_filters": {
            "all_of": [
              { "test": "is_owner", "subject": "other", "value": true }
            ]
          },
          "event": "thlmm:onfs"
        }
      },
      "status:follow_standard":{
        "minecraft:behavior.follow_owner": {
          "priority": 6,
          "speed_multiplier": 1.0,
          "start_distance": 10,
          "stop_distance": 2,
          "can_teleport": true
        }
      },
      "status:home":{
        "minecraft:is_baby": {},
        "minecraft:timer": {
          "time": 3,
          "looping": true,
          "time_down_event": {
            "event": "thlmm:rh",
            "target": "self"
          }
        }
      },
      "mode:idle":{
        "minecraft:variant": {"value": 0},
        "minecraft:behavior.panic": {
          "priority": 1,
          "speed_multiplier": 1.25
        }
      },
      "mode:attack":{
        "minecraft:variant": {"value": 1},
        "minecraft:attack": {
          "damage": 4
        },
        "minecraft:behavior.melee_attack": {
          "priority": 5
        },
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 3,
          "must_reach": true,
          "must_see": true,
          "entity_types": [
            {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "monster"
                  },
                  {
                    "test": "is_family",
                    "subject": "other",
                    "operator": "!=",
                    "value": "creeper"
                  }
                ]
              },
              "within_default": 10
            },
            {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "hoglin"
                  },
                  {
                    "test": "is_difficulty",
                    "operator": "!=",
                    "value": "peaceful"
                  }
                ]
              },
              "max_dist": 16
            },
            {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "zoglin"
                  },
                  {
                    "test": "is_difficulty",
                    "operator": "!=",
                    "value": "peaceful"
                  }
                ]
              },
              "max_dist": 16
            }
          ]
        },
        "minecraft:behavior.owner_hurt_by_target": {
          "priority": 1
        },
        "minecraft:behavior.owner_hurt_target": {
          "priority": 2
        }
      }
    },

