{
  "format_version": "1.18.10",
  "minecraft:entity": {
    "description": {
      "identifier": "thlmm:maid_basic",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },

    "component_groups": {
      "despawn":{ "minecraft:instant_despawn":{ "remove_child_entities": false } },
      "thlmm:maid_wild": {
        "minecraft:behavior.panic": {
          "priority": 1,
          "speed_multiplier": 1.25
        },
        "minecraft:tameable": {
          "probability": 0.8,
          "tame_items": "cake",
          "tame_event": {
            "event": "on_tamed",
            "target": "self"
          }
        }
      },
      "thlmm:maid_tame": {
        "minecraft:sittable": {},
        "minecraft:inventory":{
          "container_type": "inventory",
          "inventory_size": 27,
          "private": false,
          "restrict_to_owner": true
        },
        "minecraft:is_tamed": {
        },
        "minecraft:damage_sensor": { 
          "triggers": [
            {
              "cause": "attack",
              "on_damage": {
                "filters": {
                  "all_of": [
                    {"test": "is_family","subject": "damager","value": "player"},
                    {"test": "is_owner","subject": "damager","value": true}
                  ]
                },
                "event": "thlmm:omi",
                "target": "self"
              },
              "deals_damage": false
            }
          ]
        },
        "minecraft:interact": {
          "interactions": [
            {
              "cooldown": 2.5,
              "use_item": false,
              "hurt_item": 1,
              "interact_text": "action.interact.camera",
              "vibration": "none",
              "on_interact": {
                "filters": {
                  "all_of": [
                    { "test": "has_equipment", "subject": "other", "domain": "hand", "value": "touhou_little_maid:camera"},
                    { "test": "is_family", "subject": "other", "value": "player" },
                    { "test": "is_owner", "subject": "other", "value": true }
                  ]
                },
                "event": "thlmm:p",
                "target": "self"
              }
            },
            {
              "cooldown": 2.5,
              "use_item": false,
              "interact_text": "action.interact.smart_slab",
              "vibration": "none",
              "on_interact": {
                "filters": {
                  "all_of": [
                    { "test": "has_equipment", "subject": "other", "domain": "hand", "value": "touhou_little_maid:smart_slab_empty"},
                    { "test": "is_family", "subject": "other", "value": "player" },
                    { "test": "is_owner", "subject": "other", "value": true }
                  ]
                },
                "event": "thlmm:ss",
                "target": "self"
              }
            }
          ]
        }
      },
      "thlmm:maid_tame_sit":{
        "minecraft:entity_sensor": {
          "sensor_range": 6,
          "relative_range": false,
          "minimum_count": 1,
          "maximum_count": 2,
          "event_filters": {
            "all_of":[
              {
                "all_of": [
                  {"test": "is_owner","subject": "other","value": true},
                  {"test": "is_sneaking","subject": "other","value": true}
                ]
              },
              { "test": "distance_to_nearest_player", "subject" : "self", "operator": "<", "value" : 7}
            ]
          },
          "event": "thlmm:i"
        }
      },
      "thlmm:maid_tame_inventory":{
        "minecraft:entity_sensor": {
          "sensor_range": 6,
          "relative_range": false,
          "minimum_count": 1,
          "maximum_count": 2,
          "event_filters": {
            "any_of":[
              {
                "all_of": [
                  {"test": "is_owner","subject": "other","value": true},
                  {"test": "is_sneaking","subject": "other","value": false},
                  {"test": "has_container_open", "subject": "other", "value": false}
                ]
              },
              { "test": "distance_to_nearest_player", "subject" : "self", "operator": ">", "value" : 7}
            ]
          },
          "event": "thlmm:s"
        }
      },

      "status:follow_on_tame":{
        "minecraft:behavior.follow_owner": {
          "priority": 1,
          "speed_multiplier": 1.0,
          "start_distance": 1,
          "stop_distance": 1,
          "can_teleport": true
        },
        "minecraft:entity_sensor": {
          "sensor_range": 1,
          "relative_range": false,
          "minimum_count": 1,
          "maximum_count": 2,
          "event_filters": {
            "all_of": [
              { "test": "is_owner", "subject": "other", "value": true }
            ]
          },
          "event": "thlmm:onfs"
        }
      },
      "status:follow_standard":{
        "minecraft:behavior.follow_owner": {
          "priority": 6,
          "speed_multiplier": 1.2,
          "start_distance": 15,
          "stop_distance": 2,
          "can_teleport": true
        }
      },
      "status:home":{
        "minecraft:is_baby": {},
        "minecraft:timer": {
          "time": 3,
          "looping": true,
          "time_down_event": {
            "event": "thlmm:rh",
            "target": "self"
          }
        }
      },
      "mode:idle":{
        "minecraft:variant": {"value": 0},
        "minecraft:behavior.panic": {
          "priority": 1,
          "speed_multiplier": 1.25
        }
      },
      "mode:attack":{
        "minecraft:variant": {"value": 1},
        "minecraft:attack": {
          "damage": 2
        },
        "minecraft:behavior.melee_attack": {
          "priority": 2
        },
        
        "minecraft:behavior.owner_hurt_by_target": {
          "priority": 1
        },
        "minecraft:behavior.owner_hurt_target": {
          "priority": 2
        }
      }
    },
    "components": {
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "must_reach": true,
        "must_see": true,
        "reselect_targets": true,
        "within_radius": 20,
        "scan_interval": 10,
        "entity_types": [
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "monster"
                }
              ]
            },
            "max_dist": 20
          }
        ]
      },
      "minecraft:on_death": {
        "event": "thlmm:d",
        "target": "self"
      },
      "minecraft:rideable": {
        "controlling_seat": 1,
        "crouching_skip_interact": true,
        "family_types": ["thlm:maid_backpack"],
        "interact_text": "",
        "pull_in_entities": false,
        "rider_can_interact": false,
        "seat_count": 1,
        "seats":[{
          "position": [0,0,0],
          "lock_rider_rotation": 1
        }]
      },
      "minecraft:balloonable": {
        "mass": 0.8
      },
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:nameable": {
        "always_show": false,
        "allow_name_tag_renaming": false
      },
      "minecraft:type_family": {
        "family": [ "maid", "mob" ]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.5
      },
      "minecraft:health": {
        "value": 40,
        "max": 40
      },
      "minecraft:movement": {
        "value": 0.3
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:behavior.look_at_player": {
        "priority": 0,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:attack": {
        "damage": 3
      },

      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:healable": {
        "items": [
          {
            "item": "porkchop",
            "heal_amount": 3
          },
          {
            "item": "cooked_porkchop",
            "heal_amount": 8
          },
          {
            "item": "fish",
            "heal_amount": 2
          },
          {
            "item": "salmon",
            "heal_amount": 2
          },
          {
            "item": "clownfish",
            "heal_amount": 1
          },
          {
            "item": "pufferfish",
            "heal_amount": 1
          },
          {
            "item": "cooked_fish",
            "heal_amount": 5
          },
          {
            "item": "cooked_salmon",
            "heal_amount": 6
          },
          {
            "item": "beef",
            "heal_amount": 3
          },
          {
            "item": "cooked_beef",
            "heal_amount": 8
          },
          {
            "item": "chicken",
            "heal_amount": 2
          },
          {
            "item": "cooked_chicken",
            "heal_amount": 6
          },
          {
            "item": "muttonRaw",
            "heal_amount": 2
          },
          {
            "item": "muttonCooked",
            "heal_amount": 6
          },
          {
            "item": "rotten_flesh",
            "heal_amount": 4
          },
          {
            "item": "rabbit",
            "heal_amount": 3
          },
          {
            "item": "cooked_rabbit",
            "heal_amount": 5
          },
          {
            "item": "rabbit_stew",
            "heal_amount": 10
          }
        ]
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.mount_pathing": {
        "priority": 1,
        "speed_multiplier": 1.25,
        "target_dist": 0,
        "track_target": true
      },
      "minecraft:behavior.stay_while_sitting": {
        "priority": 3
      },
      
      "minecraft:behavior.random_stroll": {
        "priority": 8,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.beg": {
        "priority": 9,
        "look_distance": 8,
        "look_time": [ 2, 4 ],
        "items": [ "cake" ]
      },

      "minecraft:behavior.hurt_by_target": {
        "priority": 3
      },
      "minecraft:conditional_bandwidth_optimization": {
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "thlmm:maid_wild"
          ]
        },
        "trigger": "thlmm:sp"
      },
      "despawn":{ "add": { "component_groups": [ "despawn" ] } },
      "on_tamed": {
        "remove": {
          "component_groups": [
            "thlmm:maid_wild"
          ]
        },
        "add": {
          "component_groups": [
            "thlmm:maid_tame",
            "status:follow_on_tame",
            "mode:idle"
          ]
        }
      },
      "thlmm:i":{
        "remove": {"component_groups": ["thlmm:maid_tame_sit"]},
        "add": {"component_groups": ["thlmm:maid_tame_inventory"]}
      },
      "thlmm:s":{
        "add": {"component_groups": ["thlmm:maid_tame_sit"]},
        "remove": {"component_groups": ["thlmm:maid_tame_inventory"]}
      },
      "thlmm:sp":{},
      "thlmm:onfs":{},
      "thlmm:omi":{},
      "thlmm:rh":{},
      "thlmm:d":{},
      "thlmm:p":{},
      "thlmm:ss":{},
      "api:follow_on_tame_over":{
        "add": {"component_groups": ["status:follow_standard", "thlmm:maid_tame_sit"]},
        "remove": {"component_groups": ["status:follow_on_tame"]}
      },

      "api:status_follow"     :{"add"   : {"component_groups": ["status:follow_standard"]}},
      "api:status_quit_follow":{"remove": {"component_groups": ["status:follow_standard"]}},
      "api:status_home"       :{"add"   : {"component_groups": ["status:home"]}},
      "api:status_quit_home"  :{"remove": {"component_groups": ["status:home"]}},
      
      "api:mode_idle"     :  {"sequence": [{"add"   : {"component_groups": ["mode:idle"]}}]},
      "api:mode_quit_idle":  {"sequence": [{"remove": {"component_groups": ["mode:idle"]}}]},
      "api:mode_attack"     :{"sequence": [{"add"   : {"component_groups": ["mode:attack"]}}]},
      "api:mode_quit_attack":{"sequence": [{"remove": {"component_groups": ["mode:attack"]}}]}
    }
  }
}