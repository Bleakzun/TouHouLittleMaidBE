{
	"format_version": "1.8.0",
	"animations": {
		"animation.touhou_little_maid.basic.blink": {
			"loop": true,
			"bones": {
				"blink": {
					"position": [0, 0, "math.mod(math.floor(q.life_time*10), 40) < 1 ? -0.05 : 0.5"]
				}
			}
		},
		"animation.touhou_little_maid.basic.wing": {
			"loop": true,
			"bones": {
				"wingLeft": {
					"rotation": [0, "-10-math.cos(q.life_time * 75) * 15", 0]
				},
				"wingRight": {
					"rotation": [0, "math.cos(q.life_time * 75) * 15 + 10", 0]
				}
			}
		},
		"animation.touhou_little_maid.basic.hair_swing":{
			"loop": true,
			"bones": {
				"hair": {
					"rotation": [0, 0, "math.sin(q.life_time * 75) * 8"]
				}
			}
		},
		"animation.touhou_little_maid.basic.walk":{
			"loop":true,
			"bones": {
				"armLeft" : {
					"rotation" : [ "variable.tcos0", 0.0, 0.0 ]
				},
				"legLeft" : {
					"rotation" : [ "variable.tcos0 * -1.4", 0.0, 0.0 ]
				},
				"armRight" : {
					"rotation" : [ "-variable.tcos0", 0.0, 0.0 ]
				},
				"legRight" : {
					"rotation" : [ "variable.tcos0 * 1.4", 0.0, 0.0 ]
				}
			}
		},
		"animation.touhou_little_maid.basic.backpack_offset":{
			"loop":true,
			"bones": {
				"backpack" : {
					"position" : [ 0, 8, 4 ]
				}
			}
		}
	}
}