				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 12,
					"texture_height": 9,
					"uv": ["math.floor(v.particle_random_1 * 4) * 3", "math.floor(v.particle_random_1 * 4) * 3"],
